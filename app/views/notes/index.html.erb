<h1>Listing notes</h1>

<table>
  <thead>
    <tr>
      <th>Bar</th>
      <th>Beat</th>
      <th>Duration</th>
      <th>Drum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.bar %></td>
        <td><%= note.beat %></td>
        <td><%= note.duration %></td>
        <td><%= note.drum %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
